{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Проекти",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true,
      "customFieldConfig": {
        "fieldRenderer": "my_custom_field_renderer"
      }
    },
    "test": {
      "type": "component",
      "repeatable": true,
      "component": "ui.button"
    },
    "secondDescription": {
      "type": "text",
      "required": true,
      "minLength": 10,
      "unique": false,
      "maxLength": 30
    },
    "reviews": {
      "type": "component",
      "repeatable": true,
      "component": "ui.reviews"
    }
  }
}
